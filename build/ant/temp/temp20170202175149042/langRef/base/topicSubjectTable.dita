&lt;topicSubjectTable&gt;
  &lt;topicSubjectHeader&gt;
    &lt;topicCell type="task"/&gt;
    &lt;subjectCell&gt;
      &lt;topicsubject keyref="goal"/&gt;
    &lt;/subjectCell&gt;
    &lt;subjectCell&gt;
      &lt;topicapply keyref="os"/&gt;
    &lt;/subjectCell&gt;
  &lt;/topicSubjectHeader&gt;
  &lt;topicSubjectRow&gt;
