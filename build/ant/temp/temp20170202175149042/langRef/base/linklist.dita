<example class="- topic/example " id="example" xtrc="example:1;38:23" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/base/linklist.dita"><title class="- topic/title " xtrc="title:3;38:30" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/base/linklist.dita">Example</title><codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:1;38:56" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/base/linklist.dita">&lt;related-links&gt;
