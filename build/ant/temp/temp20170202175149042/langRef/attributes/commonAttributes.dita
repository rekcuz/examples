     <dt class="- topic/dt " xtrc="dt:8;74:10" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/attributes/commonAttributes.dita"><xmlatt class="+ topic/keyword markup-d/markupname xml-d/xmlatt " xtrc="xmlatt:8;74:18" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/attributes/commonAttributes.dita">anchorref</xmlatt></dt>
     <dd class="- topic/dd " xtrc="dd:8;75:10" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/attributes/commonAttributes.dita"><indexterm class="- topic/indexterm " xtrc="indexterm:16;75:21" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/langRef/attributes/commonAttributes.dita">anchorref
