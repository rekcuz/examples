  &lt;topicref href="intro.dita"/&gt;
  &lt;topicref href="install.dita"&gt;
    &lt;ditavalref href="novice.ditaval"/&gt;
    &lt;topicref href="do-stuff.dita"/&gt;
    &lt;topicref href="advanced-stuff.dita" audience="admin"/&gt;
    &lt;!-- more topics --&gt;
