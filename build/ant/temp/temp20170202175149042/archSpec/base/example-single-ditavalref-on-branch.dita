          <xmlelement class="+ topic/keyword markup-d/markupname xml-d/xmlelement " xtrc="xmlelement:3;10:23" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/archSpec/base/example-single-ditavalref-on-branch.dita">ditavalref</xmlelement> element:</p>
      <fig class="- topic/fig " id="fig_bcv_nhj_ln" xtrc="fig:1;11:32" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/archSpec/base/example-single-ditavalref-on-branch.dita">
        <title class="- topic/title " xtrc="title:2;12:16" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/archSpec/base/example-single-ditavalref-on-branch.dita"><filepath class="+ topic/ph sw-d/filepath " xtrc="filepath:1;12:26" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/archSpec/base/example-single-ditavalref-on-branch.dita">input.ditamap:</filepath></title>
        <codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:1;13:20" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/archSpec/base/example-single-ditavalref-on-branch.dita">
