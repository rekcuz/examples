   <image class="- topic/image " href="../../images/Keyscopes-Illustrated.png" placement="inline" xtrc="image:1;56:58" xtrf="file:/Users/joe.zucker/git/examples/source/dita-spec-1-3/archSpec/base/example-nested-key-scopes.dita" dita-ot:image-width="595" dita-ot:image-height="435" dita-ot:horizontal-dpi="150" dita-ot:vertical-dpi="150"/>
